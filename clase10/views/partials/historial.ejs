<style>
	.btn{
	
	padding:10px;
	background-color: purple;
	color: white;
	margin-top: 30px;
	border: none;
	border-radius: 4px;
	
}
	h1{
		font-size: 20px;
		font-weight: 100;
		color: purple;

	}

	tr,td,th{
		border-bottom: 1px solid rgb(199, 199, 199);
		text-align: left;
		

	}
	th{
		background-color: rgb(51, 13, 87);
		color: white;
		padding:5px;
		
	}
	table{
		width: 100%;
	}

	img{
		width: 30px;
		height: 30px;
		padding:3px;
	}
	

</style>
<h1>Historial de productos</h1>
<br>

<% if(fileData) { %>
<table cellspacing="0" border="0">
	<tr>
	  
	  <th id="title">Producto</th>
	  <th id="price">Precio</th>
	  <th id="thumbnail">Foto</th>
	  <th id="id">ID</th>
	</tr>
	<% fileData.forEach((producto) => { %>
		<tr>
			
			<td><%= producto.title %></td>
			<td>$<%= producto.price %></td>
			<td><img src=<%= producto.thumbnail %> alt="not found"></td>
			<td><%= producto.id %></td>
		</tr>
		<% }) %>
  </table>
  <button class="btn" onclick="window.open('/')">NUEVO PRODUCTO</button>
        </div>
        <% } else{ %>
            <h3 class="alert alert-warning">No existen datos</h3>
            <% } %>

